{
  "name": "studio",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "sanity build",
    "check-types": "tsc --noEmit",
    "clean": "rimraf node_modules .turbo .sanity coverage",
    "deploy": "sanity deploy",
    "deploy:graphql": "sanity graphql deploy",
    "dev": "sanity dev",
    "format": "prettier --write \"src/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "format:check": "prettier --check \"src/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "sanity start",
    "test": "echo 'No tests configured for studio app'",
    "test:coverage": "echo 'No tests configured for studio app'",
    "test:run": "echo 'No tests configured for studio app'",
    "test:run:coverage": "echo 'No tests configured for studio app'",
    "test:ui": "echo 'No tests configured for studio app'",
    "type": "sanity schema extract --enforce-required-fields && sanity typegen generate"
  },
  "dependencies": {
    "@guyromellemagayano/logger": "workspace:*",
    "@sanity/assist": "^4.4.0",
    "@sanity/client": "^7.6.0",
    "@sanity/icons": "^3.7.4",
    "@sanity/orderable-document-list": "^1.3.4",
    "@sanity/ui": "^2.16.2",
    "@sanity/vision": "^3.95.0",
    "lucide-react": "^0.523.0",
    "sanity": "^3.95.0",
    "sanity-plugin-asset-source-unsplash": "^3.0.3",
    "sanity-plugin-icon-picker": "^4.0.0",
    "sanity-plugin-media": "^3.0.3",
    "slugify": "^1.6.6",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@packages/eslint-config": "workspace:*",
    "@packages/typescript-config": "workspace:*",
    "@portabletext/block-tools": "^1.1.32",
    "@sanity/eslint-config-studio": "^5.0.2",
    "@sanity/schema": "^3.95.0",
    "@supercharge/promise-pool": "^3.2.0",
    "@types/jsdom": "^21.1.7",
    "dotenv": "^16.6.0",
    "jsdom": "^26.1.0"
  },
  "peerDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "bunchee": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:"
  }
