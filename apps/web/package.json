{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build",
    "check-types": "tsc --noEmit",
    "clean": "rimraf node_modules .turbo .next coverage",
    "dev": "next dev -p 3002 --turbopack",
    "format": "prettier --write \"src/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "format:check": "prettier --check \"src/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "next start",
    "test": "echo 'No tests configured for web app'",
    "test:coverage": "echo 'No tests configured for web app'",
    "test:run": "echo 'No tests configured for web app'",
    "test:run:coverage": "echo 'No tests configured for web app'",
    "test:ui": "echo 'No tests configured for web app'"
  },
  "dependencies": {
    "@guyromellemagayano/components": "workspace:*",
    "@guyromellemagayano/logger": "workspace:*",
    "@guyromellemagayano/ui": "workspace:*",
    "next": "catalog:"
  },
  "devDependencies": {
    "@packages/eslint-config": "workspace:*",
    "@packages/typescript-config": "workspace:*"
  },
  "peerDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "bunchee": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:"
  }
