  "scripts": {
    "build": "remix build",
    "check-types": "remix typegen && tsc --noEmit",
    "clean": "rimraf node_modules .turbo build coverage",
    "dev": "remix dev",
    "format": "prettier --write \"app/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "format:check": "prettier --check \"app/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "lint": "eslint app/ --max-warnings 0",
    "lint:fix": "eslint --fix app/ --max-warnings 0",
    "start": "remix-serve build/index.js",
    "test": "echo 'No tests configured for blog app'",
    "test:coverage": "echo 'No tests configured for blog app'",
