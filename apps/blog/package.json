    "test:run:coverage": "echo 'No tests configured for blog app'",
    "test:ui": "echo 'No tests configured for blog app'"
  },
  "dependencies": {
    "@guyromellemagayano/components": "workspace:*",
    "@guyromellemagayano/logger": "workspace:*",
    "@guyromellemagayano/ui": "workspace:*",
