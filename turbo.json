{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalDependencies": [
    ".env*"
  ],
  "globalEnv": [
    "AWS_ACCESS_KEY_ID",
    "AWS_REGION",
    "AWS_SECRET_ACCESS_KEY",
    "BUNDLE_ANALYZE",
    "DATADOG_API_KEY",
    "ENABLE_EXPERIMENTAL_COREPACK",
    "GOOGLE_ADSENSE_CLIENT_ID",
    "GOOGLE_ANALYTICS_MEASUREMENT_ID",
    "GOOGLE_TAG_MANAGER_CONTAINER_ID",
    "LOGROCKET_APP_ID",
    "LOGROCKET_TOKEN",
    "NEWRELIC_LICENSE_KEY",
    "NEXT_PUBLIC_PORTFOLIO_SITE_GRAPHQL_API_PATHNAME",
    "NEXT_PUBLIC_PORTFOLIO_SITE_URL",
    "NODE_ENV",
    "GOOGLE_CLOUD_PROJECT_ID",
    "GOOGLE_CLOUD_KEY_FILE",
    "AZURE_INSTRUMENTATION_KEY",
    "ELASTICSEARCH_URL",
    "ELASTICSEARCH_API_KEY",
    "SPLUNK_HEC_TOKEN",
    "SPLUNK_HOST",
    "LOCAL_ELASTICSEARCH",
    "SPLUNK_PORT",
    "PAPERTRAIL_HOST",
    "PAPERTRAIL_PORT",
    "SANITY_DEPLOY_TOKEN",
    "SANITY_STUDIO_DATASET",
    "SANITY_STUDIO_PRESENTATION_URL",
    "SANITY_STUDIO_PREVIEW_URL",
    "SANITY_STUDIO_PROJECT_ID",
    "SANITY_STUDIO_STEGA_ENABLED",
    "SANITY_STUDIO_TITLE",
    "SANITY_STUDIO_URL",
    "SENTRY_AUTH_TOKEN",
    "SENTRY_DSN",
    "SENTRY_ENVIRONMENT",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_SUPPRESS_GLOBAL_ERROR_HANDLER_FILE_WARNING",
    "VERCEL_ENV",
    "VERCEL_PROJECT_PRODUCTION_URL",
    "VERCEL_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "build/**",
        "dist/**",
        ".next/**",
        "!.next/cache/**",
        ".vercel/**",
        ".sanity/**"
      ]
    },
    "test": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "vitest.config.*",
        "src/test-setup.*"
      ],
      "outputs": [],
      "cache": false
    },
    "test:ui": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "vitest.config.*",
        "src/test-setup.*"
      ],
      "outputs": [],
      "cache": false,
      "persistent": true
    },
    "test:run": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "vitest.config.*",
        "src/test-setup.*"
      ],
      "outputs": [],
      "cache": true
    },
    "test:coverage": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "vitest.config.*",
        "src/test-setup.*"
      ],
      "outputs": [
        "coverage/**"
      ],
      "cache": false
    },
    "test:coverage:ui": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "vitest.config.*",
        "src/test-setup.*"
      ],
      "outputs": [
        "coverage/**"
      ],
      "cache": false,
      "persistent": true
    },
    "test:run:coverage": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "vitest.config.*",
        "src/test-setup.*"
      ],
      "outputs": [
        "coverage/**"
      ],
      "cache": true
    },
    "format": {
      "dependsOn": [
        "^format"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "prettier.config.*"
      ],
      "outputs": []
    },
    "format:check": {
      "dependsOn": [
        "^format:check"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "prettier.config.*"
      ],
      "outputs": [],
      "cache": false
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "eslint.config.*"
      ],
      "outputs": []
    },
    "lint:fix": {
      "dependsOn": [
        "^lint:fix"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "eslint.config.*"
      ],
      "outputs": [],
      "cache": false
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "tsconfig.json"
      ],
      "outputs": []
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "persistent": true,
      "cache": false
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ],
      "outputs": [],
      "cache": false
    }
  }
}
