{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": [
    "NODE_ENV",
    "VERCEL_ENV",
    "VERCEL_URL",
    "VERCEL_PROJECT_PRODUCTION_URL",
    "BUNDLE_ANALYZE",
    "ENABLE_EXPERIMENTAL_COREPACK",
    "GOOGLE_ADSENSE_CLIENT_ID",
    "GOOGLE_ANALYTICS_MEASUREMENT_ID",
    "GOOGLE_TAG_MANAGER_CONTAINER_ID",
    "NEXT_PUBLIC_PORTFOLIO_SITE_GRAPHQL_API_PATHNAME",
    "NEXT_PUBLIC_PORTFOLIO_SITE_URL",
    "SANITY_DEPLOY_TOKEN",
    "SANITY_STUDIO_PROJECT_ID",
    "SANITY_STUDIO_DATASET",
    "SANITY_STUDIO_TITLE",
    "SANITY_STUDIO_PRESENTATION_URL",
    "SENTRY_AUTH_TOKEN",
    "SENTRY_DSN",
    "SENTRY_ENVIRONMENT",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_SUPPRESS_GLOBAL_ERROR_HANDLER_FILE_WARNING"
  ],
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**", ".next/**", ".sanity/**", ".vercel/**", "!.next/cache/**"]
    },
    "test": {
      "inputs": ["$TURBO_DEFAULT$"],
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "inputs": ["$TURBO_DEFAULT$", ".eslintrc*", "eslint.config.*", "tsconfig.json"],
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "check-types": {
      "inputs": ["$TURBO_DEFAULT$", "tsconfig.json"],
      "dependsOn": ["^check-types"],
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "format": {
      "inputs": ["$TURBO_DEFAULT$", "prettier.config.*", ".prettierrc*"],
      "dependsOn": [],
      "outputs": []
    }
  }
}
