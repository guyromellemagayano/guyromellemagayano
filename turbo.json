{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": [
    "SANITY_API_READ_TOKEN",
    "SANITY_API_WRITE_TOKEN",
    "VERCEL_URL",
    "VERCEL_PROJECT_PRODUCTION_URL",
    "VERCEL_ENV",
    "NODE_ENV"
  ],
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**", ".next/**", ".vercel/**", ".sanity/**", "!.next/cache/**"]
    },
    "test": {
      "inputs": ["$TURBO_DEFAULT$"],
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "inputs": ["$TURBO_DEFAULT$", ".eslintrc*", "eslint.config.*", "tsconfig.json"],
      "dependsOn": ["^lint"],
      "outputs": []
    },
    "check-types": {
      "inputs": ["$TURBO_DEFAULT$", "tsconfig.json"],
      "dependsOn": ["^check-types"],
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "format": {
      "inputs": ["$TURBO_DEFAULT$", "prettier.config.*", ".prettierrc*"],
      "outputs": [],
      "dependsOn": []
    }
  }
}
