{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vite - TypeScript",
  "extends": "./base.json",
  "compilerOptions": {
    "useDefineForClassFields": true,
    "jsx": "react-jsx",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "src"
  ]
}
