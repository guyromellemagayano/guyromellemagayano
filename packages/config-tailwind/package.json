{
  "name": "@packages/tailwind-config",
  "version": "1.0.0",
  "private": true,
  "description": "Shared TailwindCSS config presets",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.js"
  },
  "main": "./src/index.js",
  "module": "./src/index.js",
  "scripts": {
    "clean": "rimraf node_modules .turbo dist",
    "format": "prettier --write \"src/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "format:check": "prettier --check \"src/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "lint": "eslint --ext .js,.cjs,.mjs,.ts,.cts,.mts src/",
    "lint:fix": "eslint --fix --ext .js,.cjs,.mjs,.ts,.cts,.mts src/"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "bunchee": "^6.5.2",
    "postcss": "^8.4.49",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
