{
  "name": "@packages/ui",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./counter-button": {
      "import": "./dist/counter-button/index.js",
      "types": "./dist/counter-button/index.d.ts"
    },
    "./link": {
      "import": "./dist/link/index.js",
      "types": "./dist/link/index.d.ts"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "build-storybook": "storybook build",
    "check-types": "tsc --noEmit",
    "clean": "rimraf node_modules .turbo dist",
    "dev": "bunchee --watch",
    "format": "prettier --write \"src/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "format:check": "prettier --check \"src/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix --ext .js,.cjs,.mjs,.ts,.cts,.mts src/",
    "storybook": "storybook dev -p 6006",
    "test": "jest"
  },
  "jest": {
    "preset": "@packages/jest-presets/browser"
  },
  "dependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "bunchee": "^6.5.3",
    "eslint": "catalog:",
    "jest": "catalog:",
    "rimraf": "^6.0.1",
    "tsup": "^8.0.2",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "eslint": "catalog:",
    "jest": "catalog:",
    "typescript": "catalog:"
  }
}
