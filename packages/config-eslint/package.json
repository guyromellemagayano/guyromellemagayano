{
  "name": "@packages/eslint-config",
  "version": "1.0.0",
  "private": true,
  "description": "Shared ESLint config presets",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.js",
    "./react": "./src/presets/react.js",
    "./next": "./src/presets/next.js"
  },
  "main": "./src/index.js",
  "module": "./src/index.js",
  "types": "./src/presets/react.d.ts",
  "scripts": {
    "clean": "rimraf node_modules .turbo dist",
    "format": "prettier --write \"src/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "format:check": "prettier --check \"src/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "lint": "eslint --ext .js,.cjs,.mjs,.ts,.cts,.mts src/",
    "lint:fix": "eslint --fix --ext .js,.cjs,.mjs,.ts,.cts,.mts src/"
  },
  "dependencies": {
    "@eslint/js": "^9.29.0",
    "@next/eslint-plugin-next": "^15.3.4",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "catalog:",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-turbo": "^2.5.4",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.2.0",
    "typescript": "catalog:",
    "typescript-eslint": "^8.34.1"
  },
  "devDependencies": {
    "bunchee": "^6.5.3",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "prettier": "^3.0.0",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
